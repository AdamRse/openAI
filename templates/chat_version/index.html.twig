{% extends 'base.html.twig' %}

{% block title %}ChatVersion index{% endblock %}

{% block body %}
    <h1>ChatVersion index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Label</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for chat_version in chat_versions %}
            <tr>
                <td>{{ chat_version.id }}</td>
                <td>{{ chat_version.label }}</td>
                <td>
                    <a href="{{ path('app_chat_version_show', {'id': chat_version.id}) }}">show</a>
                    <a href="{{ path('app_chat_version_edit', {'id': chat_version.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_chat_version_new') }}">Create new</a>
{% endblock %}
